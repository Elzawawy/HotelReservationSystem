<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>All Users</title>
</head>
<body>
    <div>
        <% if (role==="Owner"){%>
            <div>
                <% include ../partials/owner_nav.ejs%>
            </div>
            <%}else if(role==="Broker"){%>
                <div>
                    <% include ../partials/broker_nav.ejs%>
                </div>
                <%}else if(role==="Customer"){%>
                    <div>
                        <%include ../partials/customer_nav.ejs%>
                    </div>
                    <%}%>
    </div>
<div>
    <%for(var i=0 ; i<users.length ; i++){%>
    <p>-Username:<%=users[i].Username%></p>
    <form method="post" action="/broker/suspendUser/<%=users[i].Username%>">
        <button type="submit">Suspend</button>
    </form>
    <%}%>
</div>
</body>
</html>